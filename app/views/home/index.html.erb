<%= link_to "Create User", new_user_registration_path%>

<table class="table table-striped table-dark table-hover table-bordered border-primary">
  <tr>
    <th></th>
    <th>User name</th>
    <th>Role</th>
    <th>Employee ID</th>
    <th>Mobile No</th>
    <th>Active</th>
  </tr>
  <tr>
    <% @users.each do |user| %>
      <td scope="col"><%= check_box("user", user.id) %></td>
      <td scope="col"><%= user.user_name %></td>
      <td scope="col"><%= user.role %></td>
      <td scope="col"><%= user.employee_id %></td>
      <td scope="col"><%= user.mobile_no %></td>
      <td scope="col"><%= user.active %></td>
    <%end%>
  </tr>  
</table>
